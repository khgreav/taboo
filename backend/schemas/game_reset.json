{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "game_reset",
  "type": "object",
  "required": ["type"],
  "additionalProperties": false,
  "properties": {
    "type": {
      "title": "Message type",
      "const": "game_reset"
    },
    "players": {
      "title": "Remaining player IDs",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    }
  }
}
