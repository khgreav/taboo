{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "error_response",
  "type": "object",
  "required": ["type", "failedType", "error", "errorCode"],
  "additionalProperties": false,
  "properties": {
    "type": {
      "title": "Message type",
      "const": "error_response"
    },
    "failedType": {
      "title": "Message type",
      "type": "string"
    },
    "error": {
      "title": "Error message",
      "type": "string",
      "minLength": 1
    },
    "errorCode": {
      "title": "Error code",
      "type": "integer",
      "minimum": 0
    }
  }
}
